<configuration>
	<listener type="Mono.WebServer.HyperFastCgi.Listener.NativeListener">
		<apphost-transport type="Mono.WebServer.HyperFastCgi.Transport.NativeTransport"></apphost-transport>
	    <protocol>InterNetwork</protocol>
	    <address>127.0.0.1</address>
	    <port>9000</port>
	</listener>
    <apphost type="Mono.WebServer.HyperFastCgi.AppHosts.AspNet.AspNetApplicationHost">
		<log level="Debug" write-to-console="true" />
		<add-trailing-slash>false</add-trailing-slash>
    </apphost>
</configuration>