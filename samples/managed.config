<configuration>
	<listener type="Mono.WebServer.HyperFastCgi.Listener.ManagedFastCgiListener">
	    <listener-transport type="Mono.WebServer.HyperFastCgi.Transport.FastCgiListenerTransport"></listener-transport>
	    <apphost-transport type="Mono.WebServer.HyperFastCgi.Transport.ManagedAppHostTransport"></apphost-transport>
	    <protocol>InterNetwork</protocol>
	    <address>127.0.0.1</address>
	    <port>9000</port>
	</listener>
    <apphost type="Mono.WebServer.HyperFastCgi.AspNetServer.AspNetApplicationHost">
		<log level="Debug" write-to-console="true" />
		<add-trailing-slash>false</add-trailing-slash>
    </apphost>
</configuration>